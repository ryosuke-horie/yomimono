{
	"name": "yomimono",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "npx @biomejs/biome check",
		"knip": "knip",
		"typecheck": "tsc --noEmit",
		"format": "npx @biomejs/biome check --write",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy:setup": "bash ./setup-deploy.sh",
		"deploy": "pnpm run deploy:setup && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"orval": "orval --config orval.config.ts"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.90.12",
		"next": "16.0.7",
		"react": "19.2.1",
		"react-dom": "19.2.1",
		"react-icons": "^5.5.0",
		"server-only": "^0.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@cloudflare/workers-types": "^4.20251205.0",
		"@opennextjs/cloudflare": "1.14.4",
		"@tailwindcss/postcss": "^4.1.17",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.10.1",
		"@types/react": "^19",
		"@types/react-dom": "19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"@vitest/coverage-v8": "^4.0.15",
		"@vitest/ui": "^4.0.15",
		"jsdom": "^27.2.0",
		"knip": "^5.71.0",
		"orval": "^7.17.0",
		"postcss": "^8",
		"postcss-load-config": "^6.0.1",
		"tailwindcss": "^4.1.17",
		"typescript": "5.9.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.15",
		"wrangler": "^4.53.0"
	},
	"overrides": {
		"@ast-grep/napi": {
			"@ast-grep/napi-linux-x64-gnu": "0.35.0"
		}
	}
}
