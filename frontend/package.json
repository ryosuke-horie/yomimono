{
	"name": "effective-yomimono",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "npx @biomejs/biome check",
		"format": "npx @biomejs/biome check --write",
		"test": "vitest",
		"test:run": "vitest run",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:report": "playwright show-report",
		"test:e2e:headless": "playwright test --headed",
		"test:e2e:specific": "playwright test --grep",
		"servers:start": "node start-servers.js",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy:setup": "bash ./setup-deploy.sh",
		"deploy": "npm run deploy:setup && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.1.1",
		"@tanstack/react-query": "^5.81.2",
		"date-fns": "^4.1.0",
		"next": "15.3.4",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-hook-form": "^7.59.0",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.5",
		"@cloudflare/workers-types": "^4.20250620.0",
		"@opennextjs/cloudflare": "1.3.1",
		"@playwright/test": "^1.53.2",
		"@tailwindcss/postcss": "^4.1.10",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.0.3",
		"@types/react": "^19",
		"@types/react-dom": "19.1.6",
		"@vitejs/plugin-react": "^4.6.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.0",
		"jsdom": "^26.1.0",
		"msw": "^2.10.2",
		"postcss": "^8",
		"postcss-load-config": "^6.0.1",
		"tailwindcss": "^4.1.6",
		"typescript": "^5.8.3",
		"undici": "^7.11.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.0",
		"wrangler": "^4.21.0"
	},
	"overrides": {
		"@ast-grep/napi": {
			"@ast-grep/napi-linux-x64-gnu": "0.35.0"
		}
	}
}
