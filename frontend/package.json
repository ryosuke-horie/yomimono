{
	"name": "yomimono",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "npx @biomejs/biome check",
		"knip": "knip",
		"typecheck": "tsc --noEmit",
		"format": "npx @biomejs/biome check --write",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy:setup": "bash ./setup-deploy.sh",
		"deploy": "pnpm run deploy:setup && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"orval": "orval --config orval.config.ts",
		"check:orval": "bash scripts/check-orval-sync.sh"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.90.21",
		"next": "16.1.6",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-icons": "^5.5.0",
		"server-only": "^0.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@cloudflare/workers-types": "^4.20260304.0",
		"@opennextjs/cloudflare": "1.17.1",
		"@tailwindcss/postcss": "^4.2.1",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.2",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.3.3",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.4",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"jsdom": "^28.1.0",
		"knip": "^5.85.0",
		"orval": "^8.4.2",
		"postcss": "^8.5.6",
		"postcss-load-config": "^6.0.1",
		"tailwindcss": "^4.2.1",
		"typescript": "5.9.3",
		"vite-tsconfig-paths": "^6.1.1",
		"vitest": "^4.0.18",
		"wrangler": "^4.68.1"
	},
	"overrides": {
		"@ast-grep/napi": {
			"@ast-grep/napi-linux-x64-gnu": "0.35.0"
		}
	}
}
